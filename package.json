{
  "name": "kcc-scan",
  "version": "0.0.1",
  "description": "KuCoin Community Chain explorer",
  "main": "index.ts",
  "directories": {
    "doc": "docs"
  },
  "private": false, 
  "scripts": {
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^14.14.41",
    "husky": "^6.0.0",
    "pinst": "^2.1.6",
    "tslint": "^6.1.3"
  }
}
